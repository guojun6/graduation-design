{
    "presets": [
        ["env", {
            "targets": {
              "browsers": ["Last 2 versions", "Chrome >= 62"]
            }
        }],
        "stage-2"
    ],
    "plugins": [
        "syntax-dynamic-import",
        ["transform-runtime", {
            "helpers": true,
            "polyfill": true,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }]
    ]
}